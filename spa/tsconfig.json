{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "es5",
    "baseUrl": "./",
    "downlevelIteration": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "paths": {
      "@pages/*": [
        "src/pages/*"
      ],
      "@router/*": [
        "src/router/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@models/*": [
        "src/models/*"
      ]
    }
  }
}
